{
  "image": "robotnik/robotnik-ros:builder-humble-devel-merge",
  "runArgs": [
    "--privileged",
    "--net=host",
    "--rm",
    "--device=/dev/ttyUSB0"
  ],

  "workspaceFolder": "/home/ros/robot_ws",
  "workspaceMount": "source=${localWorkspaceFolder},target=/home/ros/robot_ws/src/vectornav,type=bind,consistency=cached",

  "extensions": [
    "ms-vscode.cpptools",
    "ms-python.python",
    "GitHub.copilot",
    "PKief.material-icon-theme",
    "redhat.vscode-xml",
    "redhat.vscode-yaml"
  ],

  "remoteUser": "ros",
  "postCreateCommand": "/home/ros/robot_ws/src/vectornav/.devcontainer/postcreate.sh"
}